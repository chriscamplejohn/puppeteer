    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Export Private Diagrams to HTML",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/export-private-diagrams-to-html.js",
            "args": ["https://structurizr.com"],
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Export Private Docs to HTML",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/export-private-documentation-to-html.js",
            "args": ["https://structurizr.com", "Ingress"],
            "console": "integratedTerminal"
        }
    ]
}